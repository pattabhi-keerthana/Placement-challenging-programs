import java.util.*;
public class Min_Max{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        ArrayList<Integer> a=new ArrayList<>();
        int i;
        for(i=0;i<n;i++)
            a.add(sc.nextInt());
        Collections.sort(a,Collections.reverseOrder());
        int k=sc.nextInt();
        System.out.println(a.get(k-1));

    }
}
